---
import Menu from './Menu.astro';

const navLinks = [
  { href: '/', label: 'Home' },
  { href: '/notes', label: 'Notes' },
  { href: '/travel', label: 'Travel' },
];
---
<header
id="relume"
class="z-[999] grid w-full grid-cols-[1fr_max-content_1fr] items-center justify-between border-b border-border-primary bg-background-primary px-[5%] md:min-h-18"
>
<!-- Mobile Menu Component -->
<Menu />

<!-- Desktop Navigation -->
<div class="flex flex-row gap-x-4">
  <a href="/" class="flex flex-shrink-0">
    <img
      src="https://d22po4pjz3o32e.cloudfront.net/logo-image.svg"
      alt="Logo image"
    />
  </a>
</div>

<!-- Social Links -->
<div class="w-full flex flex-row min-h-16 items-center justify-end gap-x-4">
    <a href="#" target="_blank">
      <img src="/images/github.svg" alt="GitHub" class="w-6 h-6" />
    </a>
    <a href="#" target="_blank">
      <img src="/images/linkedin.svg" alt="LinkedIn" class="w-6 h-6" />
    </a>
</div>
</header>